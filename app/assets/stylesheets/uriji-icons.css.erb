//= depend_on_asset "uriji-icons.svg"
@font-face {
  font-family: 'UrijiIcons';
  src:  url('<%= font_path("uriji-icons.eot") %>?nxnmyk');
  src:  url('<%= font_path("uriji-icons.eot") %>?nxnmyk#iefix') format('embedded-opentype'),
    url('<%= font_path("uriji-icons.ttf") %>?nxnmyk') format('truetype'),
    url('<%= font_path("uriji-icons.woff") %>?nxnmyk') format('woff'),
    url('<%= font_path("uriji-icons.svg") %>?nxnmyk#uriji-icons') format('svg');
  font-weight: normal;
  font-style: normal;
  font-display: block;
}

[class^="uj-"], [class*=" uj-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'UrijiIcons' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

