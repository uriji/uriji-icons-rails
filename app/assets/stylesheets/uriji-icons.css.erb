@font-face {
  font-family: 'UrijiIcons';
  font-weight: normal;
  font-style: normal;
  src:  url('<%= font_path("uriji-icons.eot") %>?bi4hc7');
  src:  url('<%= font_path("uriji-icons.eot") %>?bi4hc7#iefix') format('embedded-opentype'),
    url('<%= font_path("uriji-icons.ttf") %>?bi4hc7') format('truetype'),
    url('<%= font_path("uriji-icons.woff") %>?bi4hc7') format('woff'),
    url('<%= font_path("uriji-icons.svg") %>?bi4hc7#uriji-icons') format('svg');
  font-display: block;
}

[class^="uj-"], [class*=" uj-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'UrijiIcons' !important;
  font-weight: normal;
  font-style: normal;
  font-variant: normal;
  line-height: 1;
  text-transform: none;
  speak: none;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

